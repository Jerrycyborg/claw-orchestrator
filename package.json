{
  "name": "claw-orchestrator",
  "version": "0.1.0",
  "description": "Multi-agent orchestration layer for OpenClaw",
  "type": "module",
  "bin": {
    "orchestrator": "./src/cli.js"
  },
  "scripts": {
    "run": "node src/cli.js run --prompt \"Implement Firestore rules\" --sync-aahp --execute",
    "auto": "node src/cli.js auto --prompt \"Implement Firestore rules\" --summary",
    "orchestrate": "bash ./scripts/orchestrate.sh",
    "setup": "bash ./scripts/setup.sh",
    "status": "node src/cli.js status",
    "aahp:check": "node src/cli.js aahp-check",
    "test": "node src/test.js"
  },
  "keywords": [
    "openclaw",
    "orchestrator",
    "multi-agent",
    "aahp"
  ],
  "license": "MIT"
}
